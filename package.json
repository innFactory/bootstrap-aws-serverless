{
  "name": "bootstrap-aws-serverless",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run check && sst start --profile=innfactory-demo",
    "build": "npm run check && sst build --profile=innfactory-demo",
    "deploy": "npm run check && sst deploy --profile=innfactory-demo",
    "remove": "sst remove --profile=innfactory-demo",
    "console": "sst console",
    "check": "tsc --noEmit --noUnusedLocals false --noUnusedParameters false && npm run lint",
    "lint": "eslint .",
    "test": "sst load-config -- vitest run",
    "codegen:build": "cd smithy-codegen && ./gradlew build",
    "codegen:clean": "cd smithy-codegen && ./gradlew clean",
    "codegen:install": "npm i --no-save smithy-codegen/build/smithyprojections/smithy-codegen/source/typescript-ssdk-codegen && npm i --no-save smithy-codegen/build/smithyprojections/smithy-codegen/source/typescript-codegen",
    "codegen": "npm run codegen:clean && npm run codegen:build && npm run codegen:install",
    "codegen:client": "cd services/test/utils/client && swagger-codegen generate -i ../../../../smithy-codegen/build/smithyprojections/smithy-codegen/source/openapi/Api.openapi.json -l typescript-fetch --additional-properties modelPropertyNaming=original",
    "codegen:client2": "rm -rf services/test/utils/client/* && openapi-generator-cli generate -i smithy-codegen/build/smithyprojections/smithy-codegen/source/openapi/Api.openapi.json -o services/test/utils/client -g typescript-fetch --additional-properties npmName=api-client,supportsES6=false,useRxJs=false,withInterfaces=true"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.3.0",
    "@aws-lambda-powertools/tracer": "^1.3.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1238.0",
    "fp-ts": "^2.13.1",
    "inversify": "^6.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.193.0",
    "@aws-smithy/server-apigateway": "^1.0.0-alpha.6",
    "@aws-smithy/server-common": "^1.0.0-alpha.6",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@serverless-stack/cli": "^1.15.16",
    "@serverless-stack/node": "^1.15.16",
    "@serverless-stack/resources": "^1.15.16",
    "@tsconfig/node16": "^1.0.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "aws-cdk-lib": "2.39.1",
    "eslint": "^8.25.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "vitest": "^0.24.3"
  },
  "workspaces": [
    "services"
  ]
}
