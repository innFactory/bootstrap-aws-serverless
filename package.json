{
  "name": "bootstrap-aws-serverless",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run checkLocal && sst start --profile=innfactory-demo",
    "build": "npm run check && sst build --profile=innfactory-demo",
    "deploy": "npm run check && sst deploy --profile=innfactory-demo",
    "deployTest": "npm run check && sst deploy --profile=innfactory-demo --stage=test",
    "removeTest": "sst remove --profile=innfactory-demo --stage=test",
    "remove": "sst remove --profile=innfactory-demo",
    "console": "sst console",
    "check": "tsc --noEmit --noUnusedLocals false --noUnusedParameters false && npm run lint",
    "checkLocal": "tsc --noEmit --noUnusedLocals false --noUnusedParameters false && npm run lintLocal",
    "test": "sst load-config --profile=innfactory-demo --stage=test -- vitest run",
    "lint": "eslint --max-warnings=0 .",
    "lintLocal": "eslint .",
    "codegen:build": "cd smithy-codegen && ./gradlew build",
    "codegen:clean": "cd smithy-codegen && ./gradlew clean && npm run codegen:clean:clients",
    "codegen:clean:clients": "rm -rf services/test/utils/client/* ",
    "codegen:install": "npm i --no-save smithy-codegen/build/smithyprojections/smithy-codegen/source/typescript-ssdk-codegen",
    "codegen": "npm run codegen:clean && npm run codegen:build && npm run codegen:install && npm run codegen:client:application",
    "codegen:client:application": "openapi-generator-cli generate -i smithy-codegen/build/smithyprojections/smithy-codegen/source/openapi/Api.openapi.json -o services/test/utils/client -g typescript-axios --additional-properties npmName=api-client,supportsES6=false,useRxJs=false,withInterfaces=true"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.4.1",
    "@aws-lambda-powertools/tracer": "^1.4.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1238.0",
    "axios": "^1.1.3",
    "fp-ts": "^2.13.1",
    "inversify": "^6.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.193.0",
    "@aws-smithy/server-apigateway": "^1.0.0-alpha.6",
    "@aws-smithy/server-common": "^1.0.0-alpha.6",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@serverless-stack/cli": "^1.15.16",
    "@serverless-stack/node": "^1.15.16",
    "@serverless-stack/resources": "^1.15.16",
    "@tsconfig/node16": "^1.0.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.11.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "aws-cdk-lib": "2.39.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "vitest": "^0.24.3"
  },
  "workspaces": [
    "services"
  ]
}
